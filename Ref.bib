@article{garciaLivingEdgeLocal2010,
  title = {Living at the {{Edge}}: {{Local}} versus {{Positional Factors}} in the {{Long}}-{{Term Population Dynamics}} of an {{Endangered Orchid}}: {{Population Dynamics}} at {{Edges}}},
  shorttitle = {Living at the {{Edge}}},
  author = {Garc{\'i}a, Mar{\'i}a B. and Go{\~n}i, Daniel and Guzm{\'a}n, David},
  year = {2010},
