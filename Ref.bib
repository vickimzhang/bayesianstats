  shorttitle = {Molecular Phylogenetics and Biogeography of the Ambush Bugs ({{Hemiptera}}},
  author = {Masonick, Paul and Michael, Amy and Frankenberg, Sarah and Rabitsch, Wolfgang and Weirauch, Christiane},
  year = {2017},
  month = sep,
