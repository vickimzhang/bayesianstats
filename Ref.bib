@article{rasmannLatitudinalPatternsPlant2011,
  title = {Latitudinal Patterns in Plant Defense: Evolution of Cardenolides, Their Toxicity and Induction Following Herbivory: {{Latitude}} and the Evolution of Plant Defense},
