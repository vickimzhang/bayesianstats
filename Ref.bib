@article{maronLOSSENEMYRESISTANCE2004,
  title = {{{LOSS OF ENEMY RESISTANCE AMONG INTRODUCED POPULATIONS OF ST}}. {{JOHN}}'{{S WORT}} ({{HYPERICUM PERFORATUM}})},
