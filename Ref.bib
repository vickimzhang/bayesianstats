@article{molina-montenegroPositiveInteractionsLichen2013,
  title = {Positive Interactions between the Lichen {{{\emph{Usnea}}}}{\emph{ Antarctica}} ({{Parmeliaceae}}) and the Native Flora in {{Maritime Antarctica}}},
  author = {{Molina-Montenegro}, Marco A. and {Ricote-Mart{\'i}nez}, Natalia and {Mu{\~n}oz-Ram{\'i}rez}, Carlos and {G{\'o}mez-Gonz{\'a}lez}, Susana and {Torres-D{\'i}az}, Cristian and {Salgado-Luarte}, Cristian and Gianoli, Ernesto},
  editor = {Woods, Kerry},
  year = {2013},
