  journal = {Journal of Ecology},
  keywords = {black cherry,invasive species,macroecology,oomycetes,plant disease,plant–soil biota interactions,Prunus serotina,Pythium,soil-borne pathogens},
  language = {en},
  number = {5}
}

@article{reinhartSpatialVariationSoilborne2009,
