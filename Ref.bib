@article{bossoModellingGeographicDistribution2013a,
  title = {Modelling Geographic Distribution and Detecting Conservation Gaps in {{Italy}} for the Threatened Beetle {{Rosalia}} Alpina},
  author = {Bosso, Luciano and Rebelo, Hugo and Garonna, Antonio P. and Russo, Danilo},
  year = {2013},
  month = apr,
  volume = {21},
