@article{priceVegetationMicrotopographyDepth1971,
  title = {Vegetation, {{Microtopography}}, and {{Depth}} of {{Active Layer}} on {{Different Exposures}} in {{Subarctic Alpine Tundra}}},
  author = {Price, Larry W.},
  year = {1971},
  month = jul,
  volume = {52},
