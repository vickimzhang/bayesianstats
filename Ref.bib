  year = {2003},
  month = mar,
  volume = {6},
  pages = {257--264},
  issn = {1461-023X, 1461-0248},
  doi = {10.1046/j.1461-0248.2003.00423.x},
  abstract = {Invasive plants can be larger and more fecund in their invasive range than in their native range, although it is unknown how often this is a result of a genetically controlled shift in traits, a plastic response to a favourable environment, or a combination thereof. Here we present data from common garden experiments that compare the size and fecundity of native and invasive California poppies, Eschscholzia californica Cham. Individuals from 20 populations, half from California (native) and half from Chile (invasive), were grown both with and without competition from other plants in a container experiment and at two field locations. There were no differences in survival between native and invasive plants at any location. We found significant increases in size and fecundity in invasive populations at two of three locations when poppies were grown without competition from other plants. Our results indicate that genetic shifts in traits have occurred in invasive populations, and that the invasive plants are better at maximizing growth and reproduction in open environments.},
  file = {C\:\\Users\\Vicki Zhang\\Documents\\UofT\\MSc\\Papers\\Invasions\\erh&eica\\leger&rice2003.pdf},
  journal = {Ecology Letters},
  language = {en},
  number = {3}
}

