@article{schultheisNoReleaseWicked2015,
  title = {No Release for the Wicked: Enemy Release Is Dynamic and Not Associated with Invasiveness},
  shorttitle = {No Release for the Wicked},
  author = {Schultheis, Elizabeth H. and Berardi, Andrea E. and Lau, Jennifer A.},
