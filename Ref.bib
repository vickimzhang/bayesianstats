@article{sagarinAbundantCentreDistribution2002,
  title = {The 'abundant Centre' Distribution: To What Extent Is It a Biogeographical Rule?},
  shorttitle = {The 'abundant Centre' Distribution},
  author = {Sagarin, Raphael D. and Gaines, Steven D.},
  year = {2002},
