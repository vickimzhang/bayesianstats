@article{porterDynamicGenomicArchitecture2019,
  title = {Dynamic Genomic Architecture of Mutualistic Cooperation in a Wild Population of {{Mesorhizobium}}},
  author = {Porter, Stephanie S. and {Faber-Hammond}, Joshua and Montoya, Angeliqua P. and Friesen, Maren L. and Sackos, Cynthia},
  year = {2019},
