@article{marionOpentopDesignsManipulating1997,
  title = {Open-Top Designs for Manipulating Field Temperature in High-Latitude Ecosystems},
  author = {Marion, G.M. and Henry, G.H.R. and Freckman, D.W. and Johnstone, J. and Jones, G. and Jones, M.H. and L{\'e}vesque, E. and Molau, U. and M\o lgaard, P. and Parsons, A.N. and {J. Svoboda} and Virginia, R.A.},
  year = {1997},
  month = dec,
  volume = {3},
  pages = {20--32},
  issn = {13541013},
  doi = {10.1111/j.1365-2486.1997.gcb136.x},
  abstract = {Passive open-top devices have been proposed as a method to experimentally increase temperature in high-latitude ecosystems. There is, however, little documentation on the efficacy of these devices. This paper examines the performance of four open-top chambers for altering temperature at six sites in the Arctic and Antarctica. Most of the heating effect was due to daytime warming above ambient; occasional night-time cooling below ambient, especially of air temperatures, depressed mean daily temperature. The mean daily temperatures at four arctic sites were generally increased by 1.2\textendash 1.8 \textdegree C; but occasionally, temperature depressions also occurred. Under optimal conditions at the antarctic site (dry soils, no vegetation, high radiation) mean daily soil temperatures were increased by ⍣2.2 \textdegree C (\textendash 10 cm) to ⍣5.2 \textdegree C (0 cm). Protection from wind may play a more important role than temperature per se in providing a favourable environment for plant growth within opentop devices. Wind speed had a generally negative impact on mean daily temperature. Daily global radiation was both positively and negatively related to chamber temperature response. The effect of chambers on snow accumulation was variable with the Alexandra Fjord site showing an increased accumulation in chambers but no difference in the date of snowmelt, while at Latnjajaure in a deep snowfall site, snowmelt occurred 1\textendash 2 weeks earlier in chambers, potentially increasing the growing season. Selection of a passive temperature-enhancing system requires balancing the temperature enhancement desired against potential unwanted ecological effects such as chamber overheating and altered light, moisture, and wind. In general, the more closed the temperature-enhancing system, the higher is the temperature enhancement, but the larger are the unwanted ecological effects. Open-top chambers alter temperature significantly and minimize most unwanted ecological effects; as a consequence, these chambers are a useful tool for studying the response of high-latitude ecosystems to warming.},
  file = {C\:\\Users\\Vicki Zhang\\Documents\\UofT\\MSc\\Papers\\Climate change\\Arctic and Subarctic\\Marion et al. (1997) - Open-top designs for manipulating field temperature in high-latitude ecosystems.pdf},
  journal = {Global Change Biology},
  keywords = {Antarctica,Arctic,climate change,ITEX,passive warming devices,temperature,tundra},
  language = {en},
  number = {S1}
}

@article{mariottePlantSoilFeedback2018,
  title = {Plant\textendash{{Soil Feedback}}: {{Bridging Natural}} and {{Agricultural Sciences}}},
  shorttitle = {Plant\textendash{{Soil Feedback}}},
  author = {Mariotte, Pierre and Mehrabi, Zia and Bezemer, T. Martijn and De Deyn, Gerlinde B. and Kulmatiski, Andrew and Drigo, Barbara and Veen, G. F. (Ciska) and {van der Heijden}, Marcel G. A. and Kardol, Paul},
  year = {2018},
  month = feb,
  volume = {33},
  pages = {129--142},
  issn = {0169-5347},
  doi = {10.1016/j.tree.2017.11.005},
  abstract = {In agricultural and natural systems researchers have demonstrated large effects of plant\textendash soil feedback (PSF) on plant growth. However, the concepts and approaches used in these two types of systems have developed, for the most part, independently. Here, we present a conceptual framework that integrates knowledge and approaches from these two contrasting systems. We use this integrated framework to demonstrate (i) how knowledge from complex natural systems can be used to increase agricultural resource-use efficiency and productivity and (ii) how research in agricultural systems can be used to test hypotheses and approaches developed in natural systems. Using this framework, we discuss avenues for new research toward an ecologically sustainable and climate-smart future.},
  file = {C\:\\Users\\Vicki Zhang\\Zotero\\storage\\3LINADRT\\Mariotte et al. - 2018 - Plant–Soil Feedback Bridging Natural and Agricult.pdf},
  journal = {Trends in Ecology \& Evolution},
  keywords = {biodiversity conservation,climate change,crop rotation,plant root traits,productivity,sustainability},
  language = {en},
  number = {2}
}

