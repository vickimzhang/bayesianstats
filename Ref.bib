@article{wubsSoilInoculationSteers2016a,
  title = {Soil Inoculation Steers Restoration of Terrestrial Ecosystems},
  author = {Wubs, E. R. Jasper and {van der Putten}, Wim H. and Bosch, Machiel and Bezemer, T. Martijn},
  year = {2016},
