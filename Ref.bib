@article{trimmerAdaptiveLearningCan2015,
  title = {Adaptive Learning Can Result in a Failure to Profit from Good Conditions: Implications for Understanding Depression},
  shorttitle = {Adaptive Learning Can Result in a Failure to Profit from Good Conditions},
  author = {Trimmer, P. C. and Higginson, A. D. and Fawcett, T. W. and McNamara, J. M. and Houston, A. I.},
