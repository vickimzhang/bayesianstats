@article{cavieresCompetitionNativeAntarctic2018,
  title = {Competition between Native {{Antarctic}} Vascular Plants and Invasive {{Poa}} Annua Changes with Temperature and Soil Nitrogen Availability},
  author = {Cavieres, Lohengrin A. and Sanhueza, Ana Karen and {Torres-Mellado}, Gustavo and {Casanova-Katny}, Ang{\'e}lica},
  year = {2018},
