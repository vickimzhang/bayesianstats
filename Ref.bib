@article{hyattComparativePopulationDynamics2006,
  title = {Comparative {{Population Dynamics}} of an {{Invading Species}} in Its {{Native}} and {{Novel Ranges}}},
  author = {Hyatt, Laura A. and Araki, Sachiko},
  year = {2006},
  month = mar,
  volume = {8},
