@article{degruchyBIOMASSPRODUCTIVITYDOMINANCE2005,
  title = {{{BIOMASS}}, {{PRODUCTIVITY}}, {{AND DOMINANCE OF ALIEN PLANTS}}: {{A MULTIHABITAT STUDY IN A NATIONAL PARK}}},
