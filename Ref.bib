@article{colauttiInvasionSuccessExplained2004a,
  ids = {colauttiInvasionSuccessExplained2004},
  title = {Is Invasion Success Explained by the Enemy Release Hypothesis?},
  author = {Colautti, Robert I. and Ricciardi, Anthony and Grigorovich, Igor A. and MacIsaac, Hugh J.},
  year = {2004},
  volume = {7},
  pages = {721--733},
  issn = {1461-0248},
  doi = {10.1111/j.1461-0248.2004.00616.x},
  abstract = {A recent trend in invasion ecology relates the success of non-indigenous species (NIS) to reduced control by enemies such as pathogens, parasites and predators (i.e. the enemy release hypothesis, ERH). Despite the demonstrated importance of enemies to host population dynamics, studies of the ERH are split \textendash{} biogeographical analyses primarily show a reduction in the diversity of enemies in the introduced range compared with the native range, while community studies imply that NIS are no less affected by enemies than native species in the invaded community. A broad review of the invasion literature implies at least eight non-exclusive explanations for this enigma. In addition, we argue that the ERH has often been accepted uncritically wherever (i) NIS often appear larger, more fecund, or somehow `better' than either congeners in the introduced region, or conspecifics in the native range; and (ii) known enemies are conspicuously absent from the introduced range. However, all NIS, regardless of their abundance or impact, will lose natural enemies at a biogeographical scale. Given the complexity of processes that underlie biological invasions, we argue against a simple relationship between enemy `release' and the vigour, abundance or impact of NIS.},
  file = {C\:\\Users\\Vicki Zhang\\Documents\\UofT\\MSc\\Papers\\Invasions\\erh&eica\\Colautti et al. (2004).pdf;C\:\\Users\\Vicki Zhang\\Documents\\UofT\\MSc\\Papers\\Invasions\\erh&eica\\colauttietal2004.pdf;C\:\\Users\\Vicki Zhang\\Zotero\\storage\\KKCUBWKB\\Colautti et al. - 2004 - Is invasion success explained by the enemy release.pdf},
  journal = {Ecology Letters},
  keywords = {Enemy inversion hypothesis,enemy of my enemy hypothesis,enemy release hypothesis,exotic species,increased susceptibility hypothesis,invasion success,non-indigenous species},
  language = {en},
  number = {8}
}

