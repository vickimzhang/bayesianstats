@article{yongNectarFeedingPredatoryAmbush2003,
  title = {Nectar-{{Feeding}} by a {{Predatory Ambush Bug}} ({{Heteroptera}}: {{Phymatidae}}) {{That Hunts}} on {{Flowers}}},
  shorttitle = {Nectar-{{Feeding}} by a {{Predatory Ambush Bug}} ({{Heteroptera}}},
  author = {Yong, Tze-hei},
  year = {2003},
  month = sep,
  volume = {96},
  pages = {643--651},
  issn = {00138746, 00138746},
  doi = {10.1603/0013-8746(2003)096[0643:NBAPAB]2.0.CO;2},
  abstract = {Ambush bugs, Phymata pennsylvanica Handlirsch (Heteroptera: Phymatidae), are generalist sit-and-wait predators commonly found hunting on old-\TH eld \ss owers in the Northeastern United States. Individuals will often probe \ss owers for long periods, suggesting that the bugs are feeding on nectar. The objective of the reported studies was to examine the ambush bug\~Os ability to detect and use simulated and real nectar as food. In a \TH eld experiment, ambush bugs that were restricted without prey on in\ss orescences survived longer than bugs restricted to non\ss owering sites, thus indicating that the bugs were able to gain nutritive material directly from \ss owers. In laboratory choice tests, individuals were more likely to drink from a dilute sucrose-solution than from a plain water solution, indicating that the bugs were able to identify nectar as a food resource. Last, individuals provided with sucrose-solution in laboratory feeding trials survived signi\TH cantly longer in the absence of prey than individuals given only plain water or nothing and also lost mass at a slower rate. However, juveniles could not molt past any instar in which they had fed on only sucrose solution. As with many other generalist predatory arthropods, then, plant-feeding in nature likely allows ambush bugs to survive longer when prey are scarce, but plant food alone is likely to be nutritionally insuf\TH cient for normal growth and development. Further research to examine patterns of prey scarcity and possible trade-offs between the consumption of nectar and the consumption of prey will help to clarify the overall role of nectar-feeding in ambush bug ecology.},
  file = {C\:\\Users\\Vicki Zhang\\Documents\\UofT\\Papers\\Yong - 2006 - Nectar-Feeding by a Predatory Ambush Bug (Heteroptera Phymatidae) That Hunts on Flowers.pdf},
  journal = {Annals of the Entomological Society of America},
  keywords = {phymata},
  language = {en},
  number = {5}
}

@article{yongPreyCaptureGeneralist2005,
  title = {Prey {{Capture}} by a {{Generalist Predator}} on {{Flowering}} and {{Nonflowering Ambush Sites}}: {{Are Inflorescences Higher Quality Hunting Sites}}?},
  shorttitle = {Prey {{Capture}} by a {{Generalist Predator}} on {{Flowering}} and {{Nonflowering Ambush Sites}}},
