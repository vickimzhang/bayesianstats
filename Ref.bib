@article{hargreavesLocalAdaptationPrimes2019,
  title = {Local Adaptation Primes Cold-edge Populations for Range Expansion but Not Warming-induced Range Shifts},
  author = {Hargreaves, Anna L. and Eckert, Christopher G.},
