@article{zenniInvasiveMelinisMinutiflora2019,
  title = {Invasive {{Melinis}} Minutiflora Outperforms Native Species, but the Magnitude of the Effect Is Context-Dependent},
  author = {Zenni, Rafael D. and Sampaio, Alexandre B. and Lima, Yara P. and {Pessoa-Filho}, Marco and Lins, Tulio C. L. and Pivello, Vania R. and Daehler, Curtis},
