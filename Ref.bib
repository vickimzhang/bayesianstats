@article{bertheauNovelInsecttreeAssociations2010,
  title = {Novel Insect-Tree Associations Resulting from Accidental and Intentional Biological `Invasions': A Meta-Analysis of Effects on Insect Fitness},
  shorttitle = {Novel Insect-Tree Associations Resulting from Accidental and Intentional Biological `Invasions'},
  author = {Bertheau, Coralie and Brockerhoff, Eckehard G. and {Roux-Morabito}, G{\'e}raldine and Lieutier, Fran{\c c}ois and Jactel, Herv{\'e}},
  year = {2010},
  month = apr,
  volume = {13},
  pages = {506--515},
  issn = {1461023X, 14610248},
  doi = {10.1111/j.1461-0248.2010.01445.x},
  abstract = {The translocation of species beyond their native range is a major threat to biodiversity. Invasions by tree-feeding insects attacking native trees and the colonization of introduced trees by native insects result in new insect\textendash tree relationships. To date there is uncertainty about the key factors that influence the outcome of these novel interactions. We report the results of a meta-analysis of 346 pairwise comparisons of forest insect fitness on novel and ancient host tree species from 31 publications. Host specificity of insects and phylogenetic relatedness between ancient and novel host trees emerged as key factors influencing insect fitness. Overall, fitness was significantly lower on novel host species than on ancient hosts. However, in some cases, fitness increased on novel hosts, mainly in polyphagous insects or when close relatives of ancient host trees were colonized. Our synthesis enables greatly improved impact prediction and risk assessment of biological invasions.},
  file = {C\:\\Users\\Vicki Zhang\\Documents\\UofT\\MSc\\Papers\\Invasions\\erh&eica\\berthatuetal2010.pdf;C\:\\Users\\Vicki Zhang\\Documents\\UofT\\Papers\\Bertheau et al. - 2010 - Novel insect-tree associations resulting from accidental and intentional biological 'invasions' A meta-analysis.pdf},
  journal = {Ecology Letters},
  language = {en},
  number = {4}
}

