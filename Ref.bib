@article{langorDiversityNonnativeTerrestrial2009,
  title = {Diversity of Non-Native Terrestrial Arthropods on Woody Plants in {{Canada}}},
  author = {Langor, David W. and DeHaas, Laura J. and Foottit, Robert G.},
  year = {2009},
